---
import getImgSrc from '@/lib/getImgSrc'

interface Props {
  src: string
  alt: string
}
const { src, alt } = Astro.props
const { env } = Astro.locals.runtime
const imgSrc = getImgSrc(env, src)
---

<!-- <img
  src={getSrc()}
  alt={alt}
  onerror=`this.onerror=null;this.src=${src}`
/> -->
<picture>
  <source src={imgSrc}>
  <img {src} {alt} >
</picture>
